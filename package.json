{
  "name": "server",
  "version": "1.0.1",
  "description": "A modern and powerful server for Node.js",
  "main": "server.js",
  "scripts": {
    "start": "node .",
    "dev": "grunt watch",
    "test": "jest --coverage --forceExit",
    "kill-comment": "Kill a process running in a specific port: PORT=3000 npm run kill",
    "kill": "kill $(lsof -t -i:$PORT) && echo '> KILLED!' || echo '> The port was already dead'"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "engineStrict": true,
  "directories": {
    "Documentation": "./docs/documentation",
    "Code": "./src",
    "Plugins": "./plugins",
    "Examples": "./examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franciscop/server"
  },
  "author": "Francisco Presencia <public@francisco.io>",
  "license": "MIT",
  "homepage": "https://serverjs.io/",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-data-parser": "^1.2.0",
    "express-session": "^1.14.2",
    "extend": "^3.0.0",
    "hbs": "^4.0.1",
    "helmet": "^3.9.0",
